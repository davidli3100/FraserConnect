{
  "expo": {
    "name": "Fraser Connect",
    "slug": "fraser-connect",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.jfss.fraserconnect",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.683024530611-kt7m86bgs5h9a54fcegdjmco6umbojs7"
        }
      },
      "supportsTablet": true
    }, 
    "android": {
      "package": "com.jfss.fraserconnect",
      "googleServicesFile": "./constants/google-services.json"
    }
  }
}